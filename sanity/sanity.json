{
  "root": true,
  "project": {
    "name": "verktoykassen"
  },
  "api": {
    "projectId": "hnbe3yhs",
    "dataset": "production"
  },
  "env": {
    "development": {
      "api": {
        "dataset": "production"
      },
      "plugins": ["@sanity/vision"]
    }
  },
  "plugins": [
    "@sanity/base",
    "@sanity/components",
    "@sanity/default-layout",
    "@sanity/default-login",
    "@sanity/desk-tool",
    "@sanity/code-input",
    "@sanity/dashboard",
    "dashboard-widget-document-list",
    "outdated-documents-widget",
    "document-in-nav-widget"
  ],
  "parts": [
    {
      "name": "part:@sanity/base/schema",
      "path": "./schemas/schema"
    },
    {
      "name": "part:@sanity/desk-tool/structure",
      "path": "./client/deskStructure.js"
    },
    {
      "implements": "part:@sanity/dashboard/config",
      "path": "./client/dashboardConfig.js"
    },
    {
      "name": "part:@sanity/base/initial-value-templates",
      "path": "./client/initialValueTemplates.js"
    },
    {
      "name": "part:@sanity/base/new-document-structure",
      "path": "./client/newDocumentStructure.js"
    },
    {
      "implements": "part:@sanity/base/document-badges/resolver",
      "path": "./client/resolveDocumentBadges.js"
    },
    {
      "implements": "part:@sanity/base/document-actions/resolver",
      "path": "./client/resolveDocumentActions.js"
    }
  ]
