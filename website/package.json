{
  "name": "website",
  "version": "0.1.0",
  "private": true,
  "description": "Nextjs page for sanity docs",
  "license": "MIT",
  "scripts": {
    "dev": "next",
    "build": "next build",
    "export": "next export",
    "start": "next start",
    "cypress": "yarn generate:testurls && cypress open",
    "cypress:ci": "yarn generate:testurls && cypress run",
    "e2e": "yarn generate:testurls && yarn build && yarn start-server-and-test start http://localhost:3000 cypress",
    "e2e:ci": "yarn generate:testurls && yarn build && yarn start-server-and-test start http://localhost:3000 cypress:ci",
    "lint": "yarn eslint components/* pages/* --ext .jsx --ext .tsx --ext .ts",
    "check:examples": "npx ts-node --project tsconfig.tsnode.json ./lib/update-documents/check-unused-code",
    "update:examples": "npx ts-node --project tsconfig.tsnode.json ./lib/update-documents/update-code-examples",
    "update:sandboxes": "npx ts-node --project tsconfig.tsnode.json ./lib/update-documents/update-code-sandboxes",
    "update:colors": "npx ts-node --project tsconfig.tsnode.json ./lib/update-documents/update-color-examples",
    "update:props": "npx ts-node --project tsconfig.tsnode.json ./lib/update-documents/update-props",
    "update:tokens": "npx ts-node --project tsconfig.tsnode.json ./lib/update-documents/tokens",
    "update:search": "npx ts-node --project tsconfig.tsnode.json ./lib/search",
    "update:sanity": "yarn update:tokens && yarn update:props && yarn update:colors && yarn update:sandboxes && yarn update:sandboxes && yarn update:examples",
    "generate:testurls": "npx ts-node --project tsconfig.tsnode.json ./lib/testing/generateTestUrls",
    "analyze": "cross-env ANALYZE=true next build",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "dependencies": {
    "@docsearch/css": "3.0.0",
    "@docsearch/react": "3.0.0",
    "@navikt/ds-css": "^0.18.7",
    "@navikt/ds-css-internal": "^0.7.5",
    "@navikt/ds-icons": "^0.8.16",
    "@navikt/ds-react": "^0.19.7",
    "@navikt/ds-react-internal": "^0.14.7",
    "@navikt/ds-tailwind": "^0.2.6",
    "@navikt/ds-tokens": "^0.9.5",
    "@sanity/block-content-to-react": "^3.0.0",
    "@sanity/client": "^3.3.0",
    "@sanity/image-url": "1.0.1",
    "@sentry/nextjs": "^6.17.6",
    "@svgr/webpack": "^5.5.0",
    "algoliasearch": "4.12.2",
    "amplitude-js": "^8.8.0",
    "canvg": "^3.0.9",
    "classnames": "^2.3.1",
    "color": "^4.0.2",
    "copy-to-clipboard": "^3.3.1",
    "date-fns": "^2.28.0",
    "elevator.js": "1.0.1",
    "github-slugger": "^1.4.0",
    "jose": "4.8.0",
    "js-file-download": "^0.4.12",
    "jszip": "^3.7.1",
    "jwt-decode": "3.1.2",
    "lodash": "^4.17.21",
    "next": "^12.1.0",
    "next-sanity": "^0.5.1",
    "prettier": "2.6.2",
    "prism-react-renderer": "^1.2.1",
    "prismjs": "^1.27.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-hotjar": "^5.0.0",
    "react-instantsearch-hooks-web": "^6.26.0",
    "react-modal": "3.14.4",
    "react-use": "^17.3.1",
    "semver": "^7.3.5",
    "sitemap": "^7.0.0",
    "tailwindcss": "^3.0.23",
    "validator": "^13.7.0"
