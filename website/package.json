  "devDependencies": {
    "@next/bundle-analyzer": "^12.1.0",
    "@sanity-codegen/types": "1.0.0-alpha.13",
    "@types/jest": "^27.4.0",
    "@types/node": "^17",
    "@types/react": "17.0.30",
    "@typescript-eslint/eslint-plugin": "^5.12",
    "@typescript-eslint/parser": "^5.12",
    "autoprefixer": "^10.4.2",
    "cross-env": "^7.0.3",
    "css": "^3.0.0",
    "cypress": "^9.1.1",
    "dotenv": "^10.0.0",
    "eslint": "^8.10",
    "eslint-config-next": "^12.1.0",
    "eslint-plugin-react": "^7.29",
    "jest": "^28.1.1",
    "next-transpile-modules": "^9.0.0",
    "postcss": "^8.4.5",
    "postcss-import": "^14.1.0",
    "sanity-codegen": "^0.9.8",
    "start-server-and-test": "^1.14.0",
    "ts-jest": "^27.1.3",
    "typescript": "^4.5"
  },
  "prettier": {}
}
