{
  "name": "storybook",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "storybook": "start-storybook -p 6006 -s public",
    "build-storybook": "build-storybook --loglevel error --quiet -s public -o ../website/public/storybook && echo Oppdaterte build",
    "storybook:watch": "watch-run -i -p 'src/**,.storybook/**' yarn build-storybook"
  },
  "dependencies": {
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.1.0",
    "@testing-library/user-event": "^13.2.1",
    "babel-plugin-require-context-hook": "^1.0.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-scripts": "4.0.3",
    "watch-run": "^1.2.5",
    "web-vitals": "^2.1.0",
    "@navikt/ds-css": "^0.8",
    "@navikt/ds-icons": "^0.5",
    "@navikt/ds-react": "0.8",
    "@navikt/ds-tokens": "^0.5"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^6.3.8",
    "@storybook/addon-essentials": "^6.3.8",
    "@storybook/addon-links": "^6.3.8",
    "@storybook/node-logger": "^6.3.8",
    "@storybook/preset-create-react-app": "^3.2.0",
    "@storybook/react": "^6.3.8"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  }
}
